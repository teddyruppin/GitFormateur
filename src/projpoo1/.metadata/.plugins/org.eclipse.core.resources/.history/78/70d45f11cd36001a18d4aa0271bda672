package spring004web.controller;

import java.util.List;

import javax.validation.Valid;

public class ModificationFormEleve {

	@Valid
	private List<ModificationEleve> listeEleves;

	public void setListeCourses(final List<ModificationEleve> plisteEleves) {
		listeEleves = plisteEleves;
	}

	public List<ModificationCourse> getListeCourses() {
		return listeCourses;
	}
}
